   "source": [
    "#program to perform svd using python\n",
    "import numpy as np\n",
    "from scipy.linalg import svd\n",
    "\n",
    "a=np.array([[1,2,3],[4,5,6],[3,2,4]])\n",
    "print(\"The matrix a is:\\n\",a)\n",
    "\n",
    "v,s,t=svd(a)  #to find svd\n",
    "\n",
    "print(\"\\n v= \",v)\n",
    "print(\"\\n s= \",s)\n",
    "print(\"\\n t= \",t)\n",
    "\n",
    "e=(v @ np.diag(s) @t)\n",
    "print(\"\\n\",e)"
   ]
